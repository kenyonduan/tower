<li>
  <%= event.strftime('%H:%M') %>
  <%= link_to event.initiator.name user_url(event.initiator), :target => '_blank' %>
  <%= show_event_action(event.action) %>
  <%= link_to target_name(event.target), target_url(event.target), :target => '_blank' %>
  <br/>
  <% render partial: 'event_body', locals: {event: event} %>
</li>